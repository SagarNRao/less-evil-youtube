{
  "manifest_version": 3,
  "name": "Less_Distracting_YouTube",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "css": ["styles.css"],
      "js": ["lib/axios.min.js", "content.js"],
      "type": "module"
    }
  ],
  "permissions": ["activeTab"]
}
